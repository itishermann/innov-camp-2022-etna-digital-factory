{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "paths": {
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],
      "@assets": ["./assets"],
      "@assets/*": ["./assets/*"],
      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"],
      "@store": ["./src/store"],
      "@store/*": ["./src/store/*"],
      "@navigation": ["./src/navigation"],
      "@navigation/*": ["./src/navigation/*"],
      "@views": ["./src/views"],
      "@views/*": ["./src/views/*"],
      "@styles": ["./src/styles"],
      "@styles/*": ["./src/styles/*"]
    }
  }
}
